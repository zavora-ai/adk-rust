{
  "agents": {
    "greeter": {
      "name": "Greeter",
      "model": "gemini-2.0-flash",
      "instructions": "You are a friendly greeter. Respond to the greeting message."
    }
  },
  "actionNodes": {
    "set_name": {
      "type": "set",
      "standard": {
        "id": "set_name",
        "name": "Set User Name",
        "description": "Sets the user name",
        "errorHandling": {"mode": "stop"},
        "tracing": {"enabled": true, "logLevel": "info"},
        "callbacks": {},
        "execution": {"timeout": 30000},
        "mapping": {"outputKey": "userName"}
      },
      "variables": [
        {"key": "userName", "value": "Alice", "valueType": "string", "isSecret": false}
      ]
    },
    "transform_greeting": {
      "type": "transform",
      "standard": {
        "id": "transform_greeting",
        "name": "Create Greeting",
        "description": "Creates a greeting message",
        "errorHandling": {"mode": "stop"},
        "tracing": {"enabled": true, "logLevel": "info"},
        "callbacks": {},
        "execution": {"timeout": 30000},
        "mapping": {"outputKey": "greeting"}
      },
      "transformType": "template",
      "expression": "Hello {{userName}}! Welcome to ADK Studio!"
    }
  },
  "workflow": {
    "type": "single",
    "edges": [
      {"from": "start", "to": "set_name"},
      {"from": "set_name", "to": "transform_greeting"},
      {"from": "transform_greeting", "to": "greeter"},
      {"from": "greeter", "to": "end"}
    ],
    "conditions": []
  }
}